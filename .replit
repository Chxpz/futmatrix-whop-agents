modules = ["python-3.11", "nodejs-20"]

[nix]
channel = "stable-25_05"
packages = ["bash", "libxcrypt", "postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AI Agents System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ai_agents_system"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Complete AI Agents System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Standalone AI System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simple AI System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AI Agents API Server"

[[workflows.workflow]]
name = "AI Agents System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install -q langgraph langchain supabase pydantic aiohttp postgrest && python main.py"

[[workflows.workflow]]
name = "ai_agents_system"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install langgraph langchain supabase aiohttp pydantic && python main.py"

[[workflows.workflow]]
name = "Complete AI Agents System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python start_system.py"
waitForPort = 5000

[[workflows.workflow]]
name = "Standalone AI System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python standalone_main.py"
waitForPort = 5000

[[workflows.workflow]]
name = "Simple AI System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python standalone_simple.py"

[[workflows.workflow]]
name = "AI Agents API Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python api_server.py"
waitForPort = 5000
